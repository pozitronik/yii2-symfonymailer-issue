{
	"type": "demo",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"php": ">=8.1",
		"yarcode/yii2-queue-mailer": "*",
		"yiisoft/yii2-symfonymailer": "^2.0",
		"yiisoft/yii2": "~2.0.0",
		"yiisoft/yii2-queue": "2.3.4"
	},
	"require-dev": {
		"yiisoft/yii2-faker": "2.0.5",
		"phpunit/phpunit": "9.5.20",
		"codeception/codeception": "4.1.31",
		"codeception/module-asserts": "1.3.1",
		"codeception/module-yii2": "1.1.5"
	},
	"config": {
		"process-timeout": 1800,
		"fxp-asset": {
			"enabled": false
		},
		"github-protocols": [
			"https"
		],
		"allow-plugins": {
			"yiisoft/yii2-composer": true
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	]
}
